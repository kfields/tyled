<?xml version="1.0" encoding="UTF-8"?>
<tileset name="{{name}}"
    tilewidth="{{tilewidth}}"
    tileheight="{{tileheight}}" 
    tilecount="{{tilecount}}" 
    columns="{{columns}}"
    margin="{{margin}}"
    spacing="{{spacing}}"
    >
<image source="{{name}}.png" width="{{width}}" height="{{height}}"/>

{% for tile in tiles %}
{% include 'tile.xml' %}
{% endfor %}

</tileset>